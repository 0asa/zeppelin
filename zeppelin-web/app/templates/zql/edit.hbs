<div class="container-fluid">
  <div class="row">
    <div id="zqlEditorHeader" class="col-md-12">
      <a data-type="text" id="zqlJobName"></a>
      <div>History <a href="#" data-type="select" id="zqlJobHistory"></a></div>
      <p />
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <div id="zqlEditor" class="zqlEditor"></div>
      {{textarea id="zqlEditorArea"}}
    </div>
  </div>
  <p />
  <div class="row">
    <div class="col-xs-12 col-sm-2 col-md-2">
      <a id="zqlRunButton" class="btn btn-lg btn-primary" {{action 'runJob'}}>Run</a>
      <a id="zqlAbortButton" class="btn btn-lg btn-primary" style="display:none;" {{action 'abortJob'}}>Abort</a>
    </div>
    <div class="col-xs-12 col-sm-4 col-md-4">
      <div id="jobMsg" style="display:inline">{{jobMessage}}</div>
    </div>
    <div class="col-xs-12 col-sm-6 col-md-6" style="float:right; text-align:right">
      Schedule - <a data-type="select2" id="zqlJobCron"></a> | 
      <a id="zqlShareButton" {{action "shareJob"}}>Share</a> | 
      <a id="zqlDeleteButton">Delete</a>
      <p style="font-size:11px">Last run {{currentJob.dateFinished}}, took {{jobTook}}</p>
    </div>
  </div>
<p />
<div id="msgBox"></div>
<div id="visualizationContainer"></div>
{{outlet}}
</div>